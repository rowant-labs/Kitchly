{
  "name": "@kitchly/plugin-kitchen",
  "version": "1.0.0",
  "description": "Kitchly - Your AI Kitchen Companion. Voice-first meal planning, recipes, and grocery ordering powered by ElizaOS and Instacart.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "elizaos start",
    "start:client": "cd frontend && bun run dev",
    "test": "bun test",
    "lint": "prettier --check src/",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@elizaos/core": "^1.0.0",
    "@elizaos/plugin-anthropic": "^1.5.12",
    "@elizaos/plugin-bootstrap": "^1.0.0",
    "@elizaos/plugin-sql": "^1.0.0",
    "@elizaos/server": "^1.7.2"
  },
  "devDependencies": {
    "@elizaos/cli": "^1.7.0",
    "@types/bun": "latest",
    "@types/node": "^22.0.0",
    "prettier": "^3.4.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "@elizaos/core": "^1.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rowant-labs/Kitchly"
  },
  "keywords": [
    "elizaos",
    "plugin",
    "kitchen",
    "recipes",
    "meal-planning",
    "instacart",
    "ai-agent",
    "voice-assistant"
  ]
}
